<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.huwei.activity">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <!-- @android:style/Theme.NoTitleBar -->
    <application
        android:name="com.huwei.application.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddProjectActivity"
            android:label="@string/title_activity_add_project" />
        <activity android:name=".NewProjectActivity" />
        <activity android:name=".ProjectInfoActivity" />
        <activity android:name=".AllFileActivity"></activity>
        <activity android:name=".CodeActivity"/>
    </application>

</manifest>
